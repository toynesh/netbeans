    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.pdsl.reports.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DowntimeTable</servlet-name>
        <servlet-class>com.pdsl.reports.DowntimeTable</servlet-class>
    </servlet>
    <servlet>
