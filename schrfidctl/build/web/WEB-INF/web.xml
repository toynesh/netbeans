<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Interpreter</servlet-name>
        <servlet-class>com.eric.rfid.Interpreter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageStudents</servlet-name>
        <servlet-class>com.eric.rfid.ManageStudents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CRUDManager</servlet-name>
        <servlet-class>com.eric.rfid.CRUDManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusReport</servlet-name>
        <servlet-class>com.eric.rfid.BusReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageUsers</servlet-name>
        <servlet-class>com.eric.rfid.ManageUsers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.eric.rfid.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LocationQuery</servlet-name>
        <servlet-class>com.eric.rfid.LocationQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Interpreter</servlet-name>
        <url-pattern>/Interpreter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageStudents</servlet-name>
        <url-pattern>/ManageStudents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CRUDManager</servlet-name>
        <url-pattern>/CRUDManager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusReport</servlet-name>
        <url-pattern>/BusReport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageUsers</servlet-name>
        <url-pattern>/ManageUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LocationQuery</servlet-name>
        <url-pattern>/LocationQuery</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
